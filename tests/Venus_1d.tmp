<problem>
planet = /data4/ananyo/models/C3M/tests/Venus_BZ2020.txt
network = venus_test.yaml
photochem = true
equilibrate = f

<init>
species = CO2, H2O, SO2, H2SO4, N2, HCl, OCS
CO2 = 0.965
SO2 = 120E-6
H2O = 120E-6
H2SO4 = 1.797E-06
N2 = 0.035
HCl = 1E-6
OCS = 1E-5

<integrator>
nSteps = 50
dt = 1E-5
Tmax = 100

<upperboundaryflux>
species = CO2, SO2
SO2 = 0.0
CO2 = 0.0

<lowerboundaryflux>
species = CO2, SO2
SO2 = 0.0
CO2 = 0.0

